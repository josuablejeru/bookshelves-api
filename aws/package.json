{
  "name": "aws",
  "version": "0.1.0",
  "bin": {
    "aws": "bin/aws.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "tsc && cdk deploy",
    "deploy:all": "npm run types:generate && tsc && npm run package && cdk deploy",
    "types:generate": "npx openapi-typescript ../openapi.yaml --output ./lambda/generated/types.ts",
    "package": "./scripts/package.sh"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "aws-cdk": "2.51.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.51.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21",
    "cdk-http-openapi": "^0.1.1",
    "@aws-cdk/aws-apigatewayv2-alpha": "2.44.0-alpha.0"
  }
}